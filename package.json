{
    "name": "vue-bootstrap4-table",
    "version": "1.1.11",
    "main": "dist/vue-bootstrap4-table.umd.js",
    "module": "dist/vue-bootstrap4-table.esm.js",
    "unpkg": "dist/vue-bootstrap4-table.min.js",
    "description": "Advanced table based on Vue 2 and Bootstrap 4",
    "author": "Rubanraj Ravichandran <rubanraj54@gmail.com>",
    "scripts": {
        "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
        "start": "npm run dev",
        "test": "mocha-webpack --webpack-config build/webpack.test.conf.js --require test/setup.js test/**/*.spec.js",
        "test:watch": "mocha-webpack --watch --webpack-config build/webpack.test.conf.js --require test/setup.js test/**/*.spec.js",
        "cover": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=html --reporter=text npm run test",
        "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
        "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-bootstrap4-table.umd.js",
        "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-bootstrap4-table.esm.js",
        "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-bootstrap4-table.min.js"
    },
    "dependencies": {
        "lodash": "^4.17.11"
    },
